{
	"main": "[[[previewurl]]]",
	"name": "[[[name]]]",
	"window": {
		"width": [[[width]]],
		"height": [[[height]]],
		"position": "center",
		"show": true
	},
	"node-remote": "[[[remotehost]]]",
	"dom_storage_quota": 50,
	"user-agent": "Mozilla/5.0 (%osinfo) AppleWebKit/%webkit_ver (KHTML, like Gecko, Chrome, Safari) NWjs/%nwver",
	"chromium-args": "--disable-plugins --disable-internal-flash --disable-popup-blocking --enable-gamepad --enable-html5-camera --allow-file-access-from-files --ignore-gpu-blacklist"
}